Robocopy "\\enx-rd-srv0002\f$\files" "D:\20201201_DATA_OT" /FFT /V /TEE /S /E /DCOPY:DA /COPY:DATS /SECFIX  /MT:128 /R:3 /W:3 /log:"D:\20201201_DATA_OT_LOG.txt"


